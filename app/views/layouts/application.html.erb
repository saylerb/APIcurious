<!DOCTYPE html>
<html>
<head>
  <title>APIcurious</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% flash.each do |name, message| %>
    <%= content_tag :div, message.html_safe, id: "flash_#{name}", class: "container flash_message" %>
  <% end %>
  <%= render 'layouts/navbar' %>

  <div class="container">
    <%= yield %>
  </div>

</body>
</html>
